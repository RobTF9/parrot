{
  "name": "parrot",
  "version": "1.0.0",
  "description": "An app for learning languages",
  "scripts": {
    "lint": "eslint client server --ext ts,tsx",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev:client": "node scripts/devServer.js",
    "dev:server": "nodemon",
    "predev": "yarn clean",
    "dev": "PORT=3000 concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:testenv": "NODE_ENV=test concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "build:client": "parcel build client/index.html --out-dir dist/client",
    "build:server": "babel server --out-dir dist/server --extensions .ts",
    "build": "rimraf dist && yarn build:server && yarn build:client",
    "start": "node dist/server/index.js",
    "reset:test": "mongo parrot-test --eval \"db.dropDatabase();\"",
    "reset:dev": "mongo parrot-dev --eval \"db.dropDatabase();\"",
    "write": "node scripts/data/write.js ",
    "seed:test": "node scripts/seed.js parrot-test",
    "preseed:dev": "yarn write",
    "seed:dev": "yarn reset:dev && node scripts/seed.js parrot-dev",
    "docs": "start-storybook -p 3001",
    "build:docs": "build-storybook",
    "new": "node scripts/component.js",
    "clean": "rimraf .cache dist",
    "pretest": "yarn write && yarn reset:test && yarn seed:test && yarn build",
    "test": "PORT=4000 NODE_ENV=test start-server-and-test 'yarn start' http://localhost:4000 'yarn cy:run'"
  },
  "author": "robtf9",
  "license": "ISC",
