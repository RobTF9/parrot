{
  "name": "parrot",
  "version": "1.0.0",
  "description": "Lanuguages library and game builder",
  "main": " index.ts",
  "scripts": {
    "lint": "eslint client server --ext ts,tsx",
    "dev:client": "parcel watch client/index.html --out-dir dist/client",
    "dev:server": "nodemon --delay 1000ms",
    "dev": "rimraf dist && concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "build:client": "parcel build client/index.html --out-dir dist/client",
    "build:server": "babel server --out-dir dist/server --extensions .ts",
    "build": "rimraf dist && yarn build:server && yarn build:client",
    "test:client": "jest --config ./jest.client.json --detectOpenHandles --verbose --coverage",
    "test:server": "jest --config ./jest.server.json --detectOpenHandles --verbose --coverage",
    "test": "yarn test:client && yarn test:server",
    "test:watch": "concurrently \"jest --config ./jest.client.json --watchAll\" \"jest --config ./jest.server.json --watchAll\"",
    "start": "node dist/server/index.js",
    "seed": "node scripts/seed.js",
    "docs": "start-storybook -p 3001",
    "build:docs": "build-storybook"
  },
  "author": "robtf9",
  "license": "ISC",
